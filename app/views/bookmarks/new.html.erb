<div class="d-flex justify-content-center">
  <div class="w-100">
    <h1>New Bookmark</h1>
    <%= simple_form_for [@list,@bookmark] do |f| %>
      <%= f.association :movie, include_blank: false %>
      <%= f.input :comment %>
      <%= f.button :submit, "Create", class: "btn btn-primary w-100" %>
    <% end %>
  </div>
</div>
