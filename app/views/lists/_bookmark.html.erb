<div class="card">
  <%= image_tag bookmark.movie.poster_url, class: "w-100"%>
  <div class="card-body">
    <h5 class="card-title"><%= bookmark.movie.title %></h5>
    <p class="card-text">
      <%= bookmark.comment %>
    </p>
    <span>
      <%= bookmark.movie.rating %>
    </span>
  </div>
</div>
